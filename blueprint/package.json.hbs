{
  "name": "{{projectName}}",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "bower install && npm run build && (cd public; if ! [ -d bower_components ]; then ln -s ../bower_components .; fi)",
    "build-templates": "rm public/templates.js ; touch public/templates.js ; if [ -d js/client/templates ]; then ember-precompile js/client/templates/*.hbs > public/templates.js; fi ",
    "build-app": "browserify js/client -o public/app.js",
    "build": "npm run build-app && npm run build-templates",
    "drop-data": "isql exec=\"SPARQL CLEAR GRAPH '{{projectURI}}'\"",
    "data-import": "(cd ./data && coffee data-import.coffee > /tmp/data.ttl) && isql exec=\"DB.DBA.TTLP (file_to_string_output ('/tmp/data.ttl'), '', '{{projectURI}}');\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node js/server"
  },
  "author": "{{author}}",
  "dependencies": {
    "eurekapi": "namlook/eurekapi",
    "bower": "~1.3.0",
    "browserify": "~3.33.0"
  },
  "license": "{{license}}",
  "devDependencies": {
    "ember-precompile": "~0.2.4"
  }
}
