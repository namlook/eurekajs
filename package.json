{
  "name": "eurekajs",
  "version": "0.0.31",
  "description": "",
  "main": "lib",
  "repository": "https://github.com/namlook/eurekajs",
  "scripts": {
    "prepublish_old": "rm -rf lib libtests ; babel src --optional runtime --out-dir lib",
    "build_old": "rm -rf lib libtests ; eslint src tests ;  babel src --optional runtime --out-dir lib --source-maps && babel tests --optional runtime --out-dir libtests --source-maps",
    "test_old": "rm -rf lib libtests ; eslint src tests 1>&2 &&  babel src --optional runtime --out-dir lib --source-maps && babel tests --optional runtime --out-dir libtests --source-maps && ./node_modules/lab/bin/lab libtests/specs -vSl",
    "watch-lint_old": "nodemon --exec 'eslint src tests' --watch src --watch tests",
    "watch-tests_old": "nodemon --exec './node_modules/lab/bin/lab libtests/specs -vl' --watch lib --watch libtests",
    "watch-babel-src_old": "babel src --watch --optional runtime --out-dir lib --source-maps",
    "watch-babel-tests_old": "babel tests --watch --optional runtime --out-dir libtests --source-maps",
    "coverage": "lab -T tests/transform.js -r html -o coverage.html tests",
    "prepublish": "npm run build",
    "lint": "eslint src tests",
    "build": "eslint src tests ; rm -r lib ; babel src --optional runtime --out-dir lib",
    "watch": "babel src --watch --optional runtime --out-dir lib --source-maps",
    "test": "eslint src tests && lab -T tests/transform.js tests/**/*-test.js -vSl",
    "test-server": "eslint src tests ; babel-node tests/app/server.js"
  },
  "bin": {
    "eureka": "bin/eureka.sh",
    "eureka-import": "bin/eureka-import.js"
  },
  "files": [
    "bin",
    "lib"
  ],
  "author": "Namlook",
  "license": "MIT",
  "dependencies": {
    "archimedes": "namlook/archimedes#v0.0.23",
    "babel-runtime": "5.8.20",
    "bcrypt": "0.8.5",
    "bluebird": "^3.2.1",
    "boom": "^3.1.2",
    "csv": "0.4.6",
    "event-stream": "^3.3.2",
    "glue": "^3.1.0",
    "gm": "^1.21.1",
    "hapi": "^12.1.0",
    "hapi-auth-basic": "^4.1.0",
    "hapi-auth-jwt": "^4.0.0",
    "hapi-mailer": "4.0.0",
    "hapi-qs": "^1.1.0",
    "inert": "^3.2.0",
    "joi": "^7.2.3",
    "jsonwebtoken": "^5.5.4",
    "kue": "^0.10.5",
    "lodash": "3.10.1",
    "mime-types": "^2.1.9",
    "nodemailer-stub-transport": "1.0.0",
    "progress": "^1.1.8",
    "shelljs": "^0.5.3",
    "socket.io": "^1.4.5",
    "stream-stream": "1.2.6",
    "yargs": "^3.30.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.23",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "babel-runtime": "^5.8.20",
    "code": "^2.1.0",
    "eslint": "^0.24.1",
    "lab": "^8.2.0",
    "require-dir": "0.3.0",
    "source-map-support": "^0.4.0"
  }
}
